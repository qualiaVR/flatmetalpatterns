<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="111019" id="com.flatmetalpatterns.app" version="1.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FlatMetalPatterns</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/36x36.png" />
    <icon density="mdpi" src="res/icon/android/48x48.png" />
    <icon density="hdpi" src="res/icon/android/72x72.png" />
    <icon density="xhdpi" src="res/icon/android/96x96.png" />
    <splash density="hdpi" src="res/screen/android/1024x783.png" />
    <splash density="ldpi" src="res/screen/android/1024x783.png" />
    <splash density="mdpi" src="res/screen/android/1024x783.png" />
    <splash density="xhdpi" src="res/screen/android/1024x783.png" />
    <splash density="hdpi" src="res/screen/android/720x1280.png" />
    <splash density="ldpi" src="res/screen/android/200x320.png" />
    <splash density="mdpi" src="res/screen/android/480x800.png" />
    <splash density="xhdpi" src="res/screen/android/720x1280.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="5000" />
</widget>
